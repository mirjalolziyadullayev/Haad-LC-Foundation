<flowgorithm fileversion="3.0">
    <attributes>
        <attribute name="name" value=""/>
        <attribute name="authors" value="asadbek"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2023-08-10 08:05:33 PM"/>
